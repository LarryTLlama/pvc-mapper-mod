{
	"schemaVersion": 1,
	"id": "pvc-mapper-mod",
	"version": "${version}",
	"name": "PVC Mapper Mod",
	"description": "Adds minimaps, full maps, search, and more from the PVC Mapper in game!",
	"authors": [
		"LarryTLlama"
	],
	"contact": {
		"homepage": "https://pvc.coolwebsite.uk/",
		"sources": "https://github.com/LarryTLlama/pvc-mapper-mod"
	},
	"license": "DWTFYW",
	"icon": "assets/pvc-mapper-mod/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			"larrytllama.pvcmappermod.PVCMapperMod"
		],
		"client": [
			"larrytllama.pvcmappermod.PVCMapperModClient"
		],
		"fabric-datagen": [
			"larrytllama.pvcmappermod.PVCMapperModDataGenerator"
		]
	},
	"mixins": [
		"pvc-mapper-mod.mixins.json",
		{
			"config": "pvc-mapper-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": "*"
	}
}